{
  "name": "Replace text in first h1",
  "description": "Replace the text content of the h1 tag with Hello world!",
  "short_name": "Replacing text",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Created extension Alexander Holub",
    "default_popup": "src/popup.html"
  },
  "icons": {
    "16": "src/img/icons/icon-16.png",
    "32": "src/img/icons/icon-32.png",
    "48": "src/img/icons/icon-48.png",
    "128": "src/img/icons/icon-128.png"
  }
}
